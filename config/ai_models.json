  "task_types": {
    "T1": "Read/Explain (understand code/roadmap/logs)",
    "T2": "Small change (≤1 file, ≤50 LOC, low risk)",
    "T3": "Medium change (2-5 files, 50-300 LOC, medium risk)",
    "T4": "Large change (≥6 files, >300 LOC, high risk)",
    "T5": "Debug/Investigate (root cause, perf analysis)",
    "T6": "Docs/Comms (PR descriptions, documentation)",
    "T7": "Standards compliance (fix logger/imports/StageIO)"
  },
  "risk_levels": {
    "low": {
      "description": "No stage boundaries, manifests, or CI changes",
      "criteria": [
        "Single file changes",
        "No manifest updates",
        "No stage output contract changes",
        "No CI/CD changes",
        "No dependency changes"
      ]
    },
    "medium": {
      "description": "Touches stage logic or multiple files",
      "criteria": [
        "2-5 file changes",
        "Stage logic modifications",
        "Multiple function changes",
        "Some output contract changes"
      ]
    },
    "high": {
      "description": "Manifests, resume logic, CI, dependencies, >10 files",
      "criteria": [
        "Manifest schema changes",
        "Resume/retry logic",
        "CI workflow changes",
        "Dependency/venv changes",
        "Bootstrap logic",
        ">10 files affected",
        "Widespread standards violations"
      ]
    }
  },
